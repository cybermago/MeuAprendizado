package cap02;

/**
 * 
 */
public class Scopes {  

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n, d, q;
		
		n = 10;
		d = 2;
		if(d != 0 && (n % d) == 0)
			System.out.println(d + "is as factor of" + n);
		d=0;
		
		if(d !=0 && (n % d) ==0)
			System.out.println(d+"is"+n);
		
		
		if(d != 0 & (n % d)==0)
			System.out.println(d+"is"+n);
		
	}

}
